                                android:layout_weight="1"
                                android:src="@drawable/tooth" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="5" >
                            </LinearLayout>

                            <ImageView
                                android:id="@+id/ImgTooth48"
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:src="@drawable/tooth" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="1" >
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1" >

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="1" >
                            </LinearLayout>

                            <ImageView
                                android:id="@+id/ImgTooth45"
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:src="@drawable/tooth" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="4" >
                            </LinearLayout>

                            <ImageView
                                android:id="@+id/ImgTooth46"
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:src="@drawable/tooth" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="1" >
